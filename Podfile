project 'Apple Device Lookup.xcodeproj'

# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Apple Device Lookup' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Apple Device Lookup
  pod 'SWXMLHash', '~> 4.0.0'
  pod 'Alamofire', '~> 4.7'

  target 'Apple Device LookupTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SWXMLHash', '~> 4.0.0'
    pod 'Alamofire', '~> 4.7'
  end

  target 'Apple Device LookupUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SWXMLHash', '~> 4.0.0'
    pod 'Alamofire', '~> 4.7'
  end

end

target 'Apple Device Lookup - Mac' do
  use_frameworks!
    # Pods for Apple Device Lookup
    pod 'SWXMLHash', '~> 4.0.0'
    pod 'Alamofire', '~> 4.7'
end
